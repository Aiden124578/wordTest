.wronglist {
  margin: 0;
  padding: 0;
}
.top {
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  text-align: center;
  border-bottom: 2rpx solid var(--themeColor);
}
.top .top-col {
  width: 100%;
  line-height: 100rpx;
  font-size: 36rpx;
  color: var(--themeColor);
}
.items {
  width: 100%;
  font-size: 32rpx;
  border-bottom: 2rpx solid #ccc;
  padding: 10rpx 5rpx;
}
.items:last-child {
  border-bottom: none;
}
.items .item-col {
  margin:0 30rpx;
  text-align: left;
  line-height: 70rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}
.word{
  color: var(--themeColor);
}